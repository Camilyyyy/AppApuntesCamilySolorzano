<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AppApuntesCamilySolorzano.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:AppApuntesCamilySolorzano.Views"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
         Title="Sobre Mi"
         ContentTemplate="{DataTemplate views:AboutPage}"
         Icon="{OnPlatform 'iconcat.png', iOS='iconcat.png', MacCatalyst='iconcat.png'}" />
        <ShellContent
            Title="Notas"
            ContentTemplate="{DataTemplate views:NotePage}"
            Icon="{OnPlatform 'iconcat.png', iOS='iconcat.png', MacCatalyst='iconcat.png'}" />
    </TabBar>

</Shell>